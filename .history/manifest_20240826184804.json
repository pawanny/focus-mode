{
    "manifest_version":3,
    "name":"Focus mode",
    "version":"1.0",
    "description":"Block distracting websites and set time limits on non-products sites.",
    "permissions":[
        "storage",
        "webRequest",
    "webRequestBlocking",
    "activeTab"
    ],
    "background":{
        "service_worker":"background.js"
    }
    ,
    "action":{
        "default_popup":"popup.html",
        "default_icon":"icons/icon-48.png"
    },
    "icons":{
        "48":"icons/icon-48.png"
    }
}-